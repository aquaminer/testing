{
  "name": "spiral/testing",
  "description": "This is my package testing",
  "keywords": [
    "spiral-packages",
    "spiral",
    "testing"
  ],
  "homepage": "https://github.com/spiral-packages/testing",
  "license": "MIT",
  "authors": [
    {
      "name": "butschster",
      "email": "butschster@gmail.com",
      "role": "Developer"
    },
    {
      "name": "roxblnfk",
      "email": "roxblnfk@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0",
    "phpunit/phpunit": "^9.5",
    "mockery/mockery": "^1.5",
    "spiral/framework": "^2.9"
  },
  "suggest": {
    "brianium/paratest": "Required to run tests in parallel (^6.0)."
  },
  "require-dev": {
    "vimeo/psalm": "^4.9"
  },
  "autoload": {
    "psr-4": {
      "Spiral\\Testing\\": "src"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
